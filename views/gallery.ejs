			<div class="row animate-box">
				<div class="col-md-8 col-md-offset-2 text-center fh5co-heading">
					<h2><%= t('Are You Attending?') %></h2>
					<p><%= t('Please Fill-up the form to notify you that you are attending. Thanks.') %></p>
				</div>
			</div>
			<div class="row animate-box">
				<div class="col-md-10 col-md-offset-1">
					<form class="form-inline">
						<!--<form action="http://localhost:3000/addAttendant" method="post" class="form-inline">-->
						<div class="col-md-3 col-sm-3">
							<div class="form-group">
								<label for="name" class="sr-only">Name</label>
								<input type="name" class="form-control" id="name" name="name" placeholder="<%= t('Name') %>" required>
							</div>
						</div>
						<div class="col-md-3 col-sm-3">
							<div class="form-group">
								<label for="participants" class="sr-only">Number</label>
								<input type="number" class="form-control" id="numberAdults" placeholder="<%= t('Number of Adults') %>" min="0" required>
							</div>
						</div>
						<div class="col-md-3 col-sm-3">
							<div class="form-group">
								<label for="participants" class="sr-only">Number</label>
								<input type="number" class="form-control" id="numberChildren" placeholder="<%= t('Number of Children') %>" min="0" required>
							</div>
						</div>
						<div class="col-md-3 col-sm-3">
							<div class="form-group">
								<label for="email" class="sr-only">Email</label>
								<input type="email" class="form-control" id="email" placeholder="<%= t('Email') %>" required>
							</div>
						</div>
						<div class="col-md-6 col-sm-6">
							<button type="submit" id="submitAttending" class="btn btn-default btn-block"><%= t('I am Attending') %></button>
						</div>
						<div class="col-md-6 col-sm-6">
							<button type="submit" id="submitNotAttending" class="btn btn-default btn-block"><%= t('I am not Attending') %></button>
						</div>
					</form>
				</div>
			</div>
